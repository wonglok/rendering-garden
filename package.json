  "scripts": {
    "start": "node main.js",
    "dev2": "xvfb-run -s \"-ac -screen 0 1080x1080x24\" nodemon main.js",
    "dev": "xvfb-run -a nodemon --ignore './dist/' main.js",
    "lin": "xvfb-run -s \"-ac -screen 0 1080x1080x24\" node main.js",
