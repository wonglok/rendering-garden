    "kill1337": "sudo kill -9 `sudo lsof -t -i:1337`",
    "pm2kill": "pm2 delete ecosystem.config.js",
    "pm2restart": "npm run pm2kill; npm run pm2create;",
    "log": "tail -f ./.cache/logs/combined.log"
  },
